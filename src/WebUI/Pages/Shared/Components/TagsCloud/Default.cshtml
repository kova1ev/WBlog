@model IEnumerable<PopularTagViewModel>

<div class="tags">
    @if (ViewBag.Message != null)
    {
        <span>@ViewBag.StatusCode</span>
        <span>@ViewBag.Message</span>
        <span>@ViewBag.StatusCode</span>
    }
    @foreach (var tag in Model ?? Enumerable.Empty<PopularTagViewModel>())
    {
        <a asp-page="Index" asp-route-tag="@tag.Name" class="badge bg-success">@tag.Name</a>
    }
</div>